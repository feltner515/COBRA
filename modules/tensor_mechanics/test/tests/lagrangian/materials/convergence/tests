[Tests]
  issues = '#17472'
  [cauchy_linear]
    type = PetscJacobianTester
    input = 'cauchy-elastic.i'
    cli_args = 'GlobalParams/large_kinematics=true'
    ratio_tol = 1E-6
    difference_tol = 1E10
    design = 'source/materials/lagrangian/ComputeLagrangianLinearElasticStress.md'
    requirement = 'Correct Jacobian coming from the material model for nonlinear kinematics  with '
                  'relative error less than 1e-6'

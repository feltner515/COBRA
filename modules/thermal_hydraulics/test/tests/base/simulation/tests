[Tests]
  [loop_identification]
    type = 'RunApp'
    input = 'loop_identification.i'
    cli_args = 'Debug/print_component_loops=true'
    expect_out = "Loop 1:
\s+corechannel
\s+hx:primary
\s+j1
\s+j2
\s+j3
\s+j4
\s+pipe1
\s+pipe2

\s+Loop 2:
\s+hx:secondary
\s+inlet
\s+outlet"
  []

